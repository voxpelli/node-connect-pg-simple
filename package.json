{
  "name": "connect-pg-simple",
  "version": "5.0.0",
  "description": "A simple, minimal PostgreSQL session store for Connect/Express",
  "url": "http://github.com/voxpelli/node-connect-pg-simple",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-connect-pg-simple.git"
  },
  "author": {
    "name": "Pelle Wessman",
    "email": "pelle@kodfabrik.se",
    "url": "http://kodfabrik.se/"
  },
  "license": "MIT",
  "dependencies": {
    "pg": "^7.4.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "index.js",
  "scripts": {
    "mocha-all": "NODE_ENV=test istanbul cover _mocha -- --exit -u exports -R spec test/*/*.spec.js test/*.spec.js",
    "mocha": "NODE_ENV=test istanbul cover _mocha -- --exit -u exports -R spec test/*.spec.js",
    "dependency-check": "JS_FILES=\"*.js test/*.js test/*/*.js\" && dependency-check . $JS_FILES && dependency-check . $JS_FILES --unused --no-dev",
    "test": "installed-check -e && eslint . && npm run --silent dependency-check && npm run --silent mocha-all",
    "test-light": "installed-check -e && eslint . && npm run --silent dependency-check && npm run --silent mocha",
    "prepush": "npm test"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "cookie-signature": "^1.1.0",
    "cookiejar": "2.1.2",
    "coveralls": "3.0.1",
    "denodeify": "1.2.1",
    "dependency-check": "3.1.0",
    "dotenv": "6.0.0",
    "escape-string-regexp": "1.0.5",
    "eslint": "4.19.1",
    "eslint-config-semistandard": "12.0.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "husky": "0.14.3",
    "installed-check": "2.1.3",
    "istanbul": "0.4.5",
    "mocha": "^5.2.0",
    "pg-promise": "^8.4.4",
    "proxyquire": "2.0.1",
    "sinon": "^6.0.0",
    "sinon-chai": "3.2.0",
    "supertest": "3.1.0"
  }
}
